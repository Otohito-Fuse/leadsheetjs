{
    "name": "leadsheetjs",
    "version": "0.0.2",
    "description": "Parse strings and transform them to information of musical lead sheets.",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/Otohito-Fuse/leadsheetjs"
    },
    "main": "build/index.js",
    "module": "build/esm/index.js",
    "types": "build/esm/index.d.ts",
    "private": false,
    "files": [
        "build"
    ],
    "keywords": [
        "music",
        "sheetmusic",
        "score",
        "leadsheet",
        "leadsheetjs",
        "chord"
    ],
    "scripts": {
        "test": "jest",
        "build": "run-p build:*",
        "build:common": "tsc",
        "build:esm": "tsc -p tsconfig.esm.json",
        "fmt": "run-p fmt:*",
        "fmt:ts": "prettier --write './**/*.{ts,tsx}'",
        "fmt:json": "prettier --write './**/*.json'"
    },
    "author": "Otohito Fuse",
    "license": "MIT",
    "devDependencies": {
        "@jest/types": "^27.5.1",
        "@types/jest": "^27.4.0",
        "@types/node": "^17.0.17",
        "jest": "^27.5.1",
        "npm-run-all": "^4.1.5",
        "prettier": "^2.5.1",
        "ts-jest": "^27.1.3",
        "ts-node": "^10.5.0",
        "typescript": "^4.5.5"
    },
    "prettier": {
        "semi": true,
        "useTabs": false,
        "tabWidth": 4
    }
}
